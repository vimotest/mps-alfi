namespace Sequences ;



class SequenceHolder {
  
  public names : String [ * ]sequence ;
  
  public rows : SeqRow [ * ]sequence ;
  
  public optionalRows : SeqRow [ * ]  ;
  
  
  public markAt (   in rowIndex: Integer  ) {
    this.rows->at ( rowIndex ).marked = true;

  }
  
  
  public optionalSpecialRow : SeqRow [ 0 .. 1 ]  ;
  
  
  public setNames (   in names: String [ * ]  ) {
    this.names = names;

  }
}